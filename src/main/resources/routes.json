[
  {
    "path": "/permission",
    "component": "layout/Layout",
    "redirect": "/permission/index",
    "alwaysShow": true,
    "meta": {
      "title": "permission",
      "icon": "lock"
    },
    "children": [
      {
        "path": "directive",
        "component": "permission/directive",
        "name": "DirectivePermission",
        "meta": {
          "title": "directivePermission"
        }
      },
      {
        "path": "role",
        "component": "permission/role",
        "name": "RolePermission",
        "meta": {
          "title": "rolePermission"
        }
      }
    ]
  },
  {
    "path": "/resource",
    "component": "layout/Layout",
    "redirect": "noRedirect",
    "children": [
      {
        "path": "/resource",
        "component": "resource/resource",
        "name": "Resource",
        "meta": {
          "title": "resource",
          "icon": "documentation"
        }
      }
    ]
  },
  {
    "path": "*",
    "redirect": "/404",
    "hidden": true
  }
]